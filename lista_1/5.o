con1 = [2 1; 1 6; 6 4; 4 5; 5 3; 3 2; 1 4; 4 3; 3 1];
con2 = [2 1; 1 6; 3 4; 1 3; 6 4; 4 5; 2 3; 3 5; 1 4];
con3 = [1 2; 6 1; 4 3; 3 1; 4 6; 4 5; 2 3; 3 5; 1 4];

function vec = f(nv,nb,con)
	vec = zeros(1,nv);
	for i = 1:nb
		p = con(i,1);
		q = con(i,2);
		vec(p) = vec(p)+1;
		vec(q) = vec(q)+1;
		disp(vec);
	end
end

f(6,9,con1)
f(6,9,con2)
f(6,9,con3)
